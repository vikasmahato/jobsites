<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="jobsite_iwm_listing_action" model="ir.actions.act_window">
        <field name="name">IWM</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">jobsite</field>
        <field name="view_mode">tree,form</field>
        <field name="context">{'default_type':'IWM'}</field>
        <field name="domain">[('type', '=', 'IWM')]</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Create Jobsites!!
            </p>
        </field>
    </record>

    <menuitem id="menu_iwm_jobsite_details"
        name="IWM"
        action="jobsite_iwm_listing_action"
        parent="menu_jobsite_sites"
        sequence="1"/>

</odoo>
